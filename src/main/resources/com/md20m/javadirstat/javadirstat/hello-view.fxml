<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="28.0" prefWidth="424.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.md20m.javadirstat.javadirstat.HelloController">
   <MenuBar prefHeight="25.0" prefWidth="285.0">
     <menus>
       <Menu mnemonicParsing="false" text="File">
         <items>
           <MenuItem mnemonicParsing="false" text="Close" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Edit">
         <items>
           <MenuItem mnemonicParsing="false" text="Delete" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Help">
         <items>
           <MenuItem mnemonicParsing="false" text="About" />
         </items>
       </Menu>
     </menus>
   </MenuBar>

    <Label fx:id="welcomeText" prefHeight="17.0" prefWidth="0.0" />
    <!--<padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>-->
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <TreeTableView prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TreeTableColumn prefWidth="75.0" text="C1" />
       <TreeTableColumn prefWidth="75.0" text="C2" />
     </columns>
   </TreeTableView>
    <ButtonBar>
        <buttons>
            <Button text="New" ButtonBar.buttonData="LEFT" />
            <Button text="Save" ButtonBar.buttonData="RIGHT" />
        </buttons>
    </ButtonBar>
   <AnchorPane prefHeight="200.0" prefWidth="200.0" />
</VBox>
